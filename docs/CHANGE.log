```xml
<configuration>
    <consoleOutput>false</consoleOutput>  <!-- Disable console spam -->
    <includeTestSourceDirectory>false</includeTestSourceDirectory>  <!-- Don't check all tests -->
    <includes>**/com/cjs/qa/junit/tests/*.java</includes>  <!-- Only new tests -->
