<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<!--
  Checkstyle Suppressions for Legacy Code
  
  These suppressions document known issues in legacy/deprecated code that:
  1. Is not actively maintained
  2. Has specific design patterns incompatible with modern standards
  3. Would require extensive refactoring to fix
  
  All new code and actively maintained code follows full Google Java Style.
-->

<suppressions>
  
  <!-- BTS (Business Technology Systems) - Legacy Insurance Application -->
  <!-- 363 violations - Primarily public page object fields (legacy design pattern) -->
  <!-- Status: Deprecated, not actively maintained -->
  <suppress checks="VisibilityModifier" files="com/cjs/qa/bts/.*\.java"/>
  <suppress checks="MemberName" files="com/cjs/qa/bts/.*\.java"/>
  
  <!-- YourMembership API Integration - Legacy XML/SOAP API -->
  <!-- 132 violations - Field naming tied to external API XML schema -->
  <!-- Status: External API contract constraints, cannot be changed -->
  <suppress checks="MemberName" files="com/cjs/qa/ym/xml/objects/dataset/.*\.java"/>
  <suppress checks="TypeName" files="com/cjs/qa/ym/xml/objects/dataset/.*\.java"/>
  
